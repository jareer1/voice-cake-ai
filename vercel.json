{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
              "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors *;"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          }
        ]
    }
  ]
}
